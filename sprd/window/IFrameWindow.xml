<window:IFrameWindowClass xmlns:window="sprd.window"
                          xmlns='http://www.w3.org/1999/xhtml'
                          xmlns:js="js.core" selected="{loaded}">
    <js:Content name="body">
        <iframe src="{src}" frameborder="0" onload="loaded()" onerror="loaded()" />
    </js:Content>
</window:IFrameWindowClass>