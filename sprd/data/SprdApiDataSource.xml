<data:SprdApiDataSourceClass xmlns:data="sprd.data" xmlns:conf="js.conf">

    <!-- optional alias, if no alias is defined alias will be determinated automatically -->

    <!-- Model can be specified via className, if className is obmitted js.data.Model is used -->
    <conf:Type path="baskets" modelClassName="sprd.model.Basket" alias="Basket"/>

    <!-- use className if you need some logic inside your models, if not use an alias and obmit the className-->
    <conf:Type path="address" alias="Address"/>
    <conf:Type path="articles" alias="Article"/>
    <conf:Type path="articleCategories" alias="ArticleCategory"/>
    <conf:Type path="applications" alias="Application"/>
    <conf:Type path="currencies" alias="Currency"/>
    <conf:Type path="countries" alias="Country"/>
    <conf:Type path="designs" alias="Design"/>
    <conf:Type path="designCategories" alias="DesignCategory"/>
    <conf:Type path="fontFamilies" alias="FontFamiliy"/>
    <conf:Type path="languages" alias="Language"/>
    <conf:Type path="printTypes" alias="PrintType"/>
    <conf:Type path="products" alias="Product"/>
    <conf:Type path="productTypes" alias="ProductType"/>
    <conf:Type path="productTypeDepartments" alias="ProductTypeDepartment"/>
    <conf:Type path="shippingTypes" alias="ShippingType"/>
    <conf:Type path="shops" alias="Shop"/>
    <conf:Type path="users" alias="User"/>

</data:SprdApiDataSourceClass>